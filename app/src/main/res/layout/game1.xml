<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <Button
        android:id="@+id/Hint"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="설명"
        android:fontFamily="@font/im_hyemin_bold"
        android:textSize="20sp"
        app:layout_editor_absoluteX="22dp"
        app:layout_editor_absoluteY="29dp"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/Stop"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="멈춤"
        android:fontFamily="@font/im_hyemin_bold"
        android:textSize="20sp"
        app:layout_editor_absoluteX="303dp"
        app:layout_editor_absoluteY="29dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/im_hyemin_bold"
        android:text="60"
        android:textColor="#0001ff"
        android:textSize="50sp"
        app:layout_editor_absoluteX="125dp"
        app:layout_editor_absoluteY="16dp"
        tools:layout_editor_absoluteX="172dp"
        tools:layout_editor_absoluteY="16dp"
        tools:ignore="MissingConstraints" />


</androidx.constraintlayout.widget.ConstraintLayout>